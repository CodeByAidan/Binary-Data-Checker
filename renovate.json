{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "platform": "github",
  "onboarding": true,
  "extends": ["config:base"],
  "automerge": true,
  "automergeType": "pr",
  "commitMessageTopic": "dependency",
  "commitMessagePrefix": "chore(deps):",
  "ignoreNodeModules": true,
  "onboardingConfig": {
    "extends": ["config:base"],
    "automerge": true
  },
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "automerge": true
    }
  ]
}
